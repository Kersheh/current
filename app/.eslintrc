{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "angular",
    "eslint:recommended"
  ],
  "rules": {
    "no-console": 0,
    "angular/log": 0
  }
}
